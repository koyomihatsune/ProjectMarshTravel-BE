{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/auth/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/auth/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/auth",
  "projects": {
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "destination": {
      "type": "application",
      "root": "apps/destination",
      "entryFile": "main",
      "sourceRoot": "apps/destination/src",
      "compilerOptions": {
        "tsConfigPath": "apps/destination/tsconfig.app.json"
      }
    },
    "trip": {
      "type": "application",
      "root": "apps/trip",
      "entryFile": "main",
      "sourceRoot": "apps/trip/src",
      "compilerOptions": {
        "tsConfigPath": "apps/trip/tsconfig.app.json"
      }
    },
    "review": {
      "type": "application",
      "root": "apps/review",
      "entryFile": "main",
      "sourceRoot": "apps/review/src",
      "compilerOptions": {
        "tsConfigPath": "apps/review/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    }
  }
}